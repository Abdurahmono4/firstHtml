{
  "builds": [{ "src": "public/**", "use": "@vercel/static" }],
  "routes": [
    { "src": "/money.html", "dest": "/public/money.html" },
    { "src": "/Dogs.html", "dest": "/public/Dogs.html" },
    { "src": "/About.html", "dest": "/public/About.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
